{
  "name": "cuestionario-mobile",
  "version": "1.0.0",
  "main": "App/index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "start:json-server": "json-server --watch db.json --port 3000",
    "start:ngrok": "npx ngrok http 3000",
    "update-env": "node updateEnv.js",
    "start-servers": "concurrently \"npm run start:json-server\" \"npm run start:ngrok\" \"npm run update-env\""
  },
  "dependencies": {
    "axios": "^1.10.0",
    "expo": "~53.0.11",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "concurrently": "^9.1.2"
  },
  "private": true
}
